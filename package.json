{
  "name": "sfn-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@nivo/bar": "^0.54.0",
    "node-sass": "^4.11.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^7.0.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "PORT=8081 react-scripts start",
    "start-test-server": "PORT=8082 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pretest": "yarn run lint",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "cypress:ci": "cypress run",
    "cypress:local": "cypress open --config baseUrl=http://localhost:8082",
    "cypress:install": "cypress install",
    "e2e:local": "start-server-and-test start-test-server http://localhost:8082 cypress:local",
    "e2e:ci": "npm-run-all --parallel --race start-test-server 'cypress:ci --config baseUrl=http://localhost:8082'"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  },
  "engines": {
    "node": "10.13.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "cypress": "^3.1.5",
    "cypress-cucumber-preprocessor": "^1.11.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.7.13"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
